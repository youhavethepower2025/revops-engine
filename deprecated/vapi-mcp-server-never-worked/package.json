{
  "name": "vapi-mcp-server",
  "version": "1.0.0",
  "description": "Multi-tenant MCP server for VAPI voice agents",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "tail": "wrangler tail",
    "d1:create": "wrangler d1 create vapi-calls-db",
    "d1:execute": "wrangler d1 execute vapi-calls-db --file=schema.sql",
    "d1:query": "wrangler d1 execute vapi-calls-db --command",
    "secret:vapi": "wrangler secret put VAPI_API_KEY"
  },
  "keywords": ["vapi", "mcp", "voice-ai", "cloudflare-workers"],
  "author": "AI Jesus Bro",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.94.0"
  }
}
