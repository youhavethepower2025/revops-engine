{
  "name": "ClearVC Amber Brain",
  "description": "Intelligent call orchestrator for ClearVC",
  "repository": "https://github.com/clearvc/amber-brain",
  "keywords": ["python", "fastapi", "retell", "ai"],
  "env": {
    "RETELL_API_KEY": {
      "description": "Your Retell API key",
      "required": true
    },
    "GHL_API_KEY": {
      "description": "ClearVC's GoHighLevel API key",
      "required": true
    },
    "GHL_LOCATION_ID": {
      "description": "ClearVC's GoHighLevel Location ID",
      "required": true
    },
    "OPENAI_API_KEY": {
      "description": "OpenAI API key for AI processing",
      "required": true
    },
    "TELEGRAM_BOT_TOKEN": {
      "description": "Telegram bot token for notifications (optional)",
      "required": false
    },
    "TELEGRAM_CHAT_ID": {
      "description": "Telegram chat ID for notifications (optional)",
      "required": false
    }
  }
}